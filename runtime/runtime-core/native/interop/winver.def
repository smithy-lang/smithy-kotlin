package = aws.smithy.kotlin.native.winver
headers = windows.h
compilerOpts = \
    -DUNICODE \
    -DWINVER=0x0601 \
    -D_WIN32_WINNT=0x0601 \
    -DWINAPI_FAMILY=3 \
    -DOEMRESOURCE \
    -Wno-incompatible-pointer-types \
    -Wno-deprecated-declarations
staticLibraries = libversion.a
libraryPaths = C:\\msys64\\mingw64\\lib
